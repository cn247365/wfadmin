              <label class="form-label"> <span class="text-danger">*</span> @Html.DisplayNameFor(model => model.AllowedAdd)</label>
              <div class="custom-control custom-checkbox">
                 <input id="@Html.IdFor(model => model.AllowedAdd)"
                       name="@Html.NameFor(model => model.AllowedAdd)"
                       value="true" @(Model.AllowedAdd ? "checked=\"checked\"" : "")
                       class="custom-control-input"
                       tabindex="6"
                       type="checkbox" />
                <label class="custom-control-label" for="@Html.IdFor(model => model.AllowedAdd)">@Html.DisplayNameFor(model => model.AllowedAdd)</label>
            </div>
      </div>

		
