﻿@model SmartAdmin.Data.Models.WfTable
  <div class="form-row">
   @Html.HiddenFor(x=>x.Id)
         <!--Catalog-->
        <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3">
            <label class="form-label"> <span class="text-danger">*</span> @Html.DisplayNameFor(model => model.Table_Catalog)</label>
			@Html.EditorFor(x => x.Table_Catalog, new { htmlAttributes = new { @class = "form-control", placeholder = Html.DisplayNameFor(x => x.Table_Catalog) } })
      
         </div>

